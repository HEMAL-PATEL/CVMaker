<?xml version="1.0" encoding="utf-8"?>
<com.otaliastudios.printer.DocumentView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_cv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical|horizontal"
    app:columnsPerPage="1"
    app:pageBackground="@android:color/white"
    app:pageDividerWidth="0.3in"
    app:pageElevation="0.03in"
    app:pagerType="vertical"
    app:printSize="ISO_A6">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            layout="@layout/layout_cv_header"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </LinearLayout>

</com.otaliastudios.printer.DocumentView>